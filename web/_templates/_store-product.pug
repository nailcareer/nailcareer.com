
extends _page.pug

block append variables
	- pageName = "store"

block append main
	.area
		.visual
			block visual
				p [visual]
		.product
			block product
				p [shopper-product]
			small.disclaimers
				p Note: depending on your location, your order may be subject to international duty fees
			.writeup
				block writeup
					p [writeup]
